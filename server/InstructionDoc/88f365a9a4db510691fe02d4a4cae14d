@startuml
entity Student{
-stud_id(PK)data:image/png;base64,
-stud_name
-stud_email
-stud_prn
-stud_gender
-stud_bdate
-stud_propic
-stud_password
-course_id (fk)
-batch_id (fk)
}

entity Faculty
{
-fac_id (pk)
-fac_name
-fac_email
-factype_id (fk)
-fac_gender
-fac_propic
-fac_bdate
-fac_password
}

entity Course{
-course_id (pk)
-course_name
}

entity Batch{
-batch_id (pk)
-batch_name
-course_id (fk)
}

Student--{Course
Student--{Batch

@enduml